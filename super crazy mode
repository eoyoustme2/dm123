-- Gộp các coroutine thành 1 hệ thống
for _, entity in ipairs(entityList) do
    coroutine.wrap(function()
        while true do
            wait(entity.delay)
            game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
            wait(0)
            pcall(function()
                loadstring(game:HttpGet(entity.url))()
            end)
        end
    end)()
end
